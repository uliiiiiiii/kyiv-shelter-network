generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Shelter {
  id         Int      @id @default(autoincrement())
  district   String
  address    String
  type       String
  subtype    String
  building   String
  owner      String
  ownership  String
  phone      String
  accessibility Boolean
  latitude   Float
  longitude  Float
  hours      String
}
